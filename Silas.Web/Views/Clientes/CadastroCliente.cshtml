@{
}
<div class="card card-custom">
    <div class="card-header">
        <h3 class="card-title">
            Cadastro de clientes
        </h3>
    </div>
    <!--begin::Form-->
    <form asp-action="CadastrarCliente" asp-controller="Clientes" method="post">
        <div class="card-body">
            @*<div class="form-group mb-8">
                <div class="alert alert-custom alert-default" role="alert">
                    <div class="alert-icon"><i class="flaticon-warning text-primary"></i></div>
                    <div class="alert-text">
                        Here are examples of <code>.form-control</code> applied to each textual HTML5 input type:
                    </div>
                </div>
            </div>*@
            <div class="form-group row">
                <label class="col-2 col-form-label">Código Cliente</label>
                <div class="col-10">
                    <input class="form-control" type="text" placeholder="12345" id="example-text-input" name="Codigo"/>
                </div>
            </div>
            <div class="form-group row">
                <label for="example-search-input" class="col-2 col-form-label">Razão Social</label>
                <div class="col-10">
                    <input class="form-control" type="text" placeholder="Auto Peças LTDA" id="example-search-input" name="RazaoSocial" />
                </div>
            </div>
            <div class="form-group row">
                <label for="example-email-input" class="col-2 col-form-label">Bairro</label>
                <div class="col-10">
                    <input class="form-control" type="text" placeholder="Vila Nova" id="example-email-input" name="Bairro" />
                </div>
            </div>
            <div class="form-group row">
                <label for="example-url-input" class="col-2 col-form-label">Cidade</label>
                <div class="col-10">
                    <input class="form-control" placeholder="São Paulo" id="example-url-input" name="Cidade" />
                </div>
            </div>
            <div class="form-group row">
                <label for="example-tel-input" class="col-2 col-form-label">Estado</label>
                <div class="col-10">
                    <input class="form-control" type="text" placeholder="SP" id="example-tel-input" name="Estado" />
                </div>
            </div>
            <div class="form-group row">
                <label for="example-password-input" class="col-2 col-form-label">Telefone</label>
                <div class="col-10">
                    <input class="form-control" type="number" placeholder="(XX)XXXXX-XXXX" id="example-password-input" name="Telefone" />
                </div>
            </div>
            <div class="form-group row">
                <label for="example-number-input" class="col-2 col-form-label">Email</label>
                <div class="col-10">
                    <input class="form-control" type="email" placeholder="autopecas@email.com" id="example-number-input" name="Email" />
                </div>
            </div>
        </div>
        <div class="card-footer">
            <div class="row">
                <div class="col-2">
                </div>
                <div class="col-10">
                    <button type="submit" class="btn btn-success mr-2" id="btn-submit">Enviar</button>
                    <button type="reset" class="btn btn-secondary">Cancelar</button>
                </div>
            </div>
        </div>
    </form>
</div>
<script src="NovoSite/src/js/pages/Teste/Teste-page.js"></script>