@{
    
}
<div id="Cliente">
    <div class="card card-custom">
        <div class="card-header">
            <h3 class="card-title">
                Dados do cliente
            </h3>
        </div>
        <!--begin::Form-->
        <form>
            <div class="card-body">
                <div class="form-group row">
                    <label class="col-2 col-form-label">Código Cliente</label>
                    <div class="col-10">
                        <output class="form-control" type="text"  id="codigo" name="Codigo" />
                    </div>
                </div>
                <div class="form-group row">
                    <label for="example-search-input" class="col-2 col-form-label">Razão Social</label>
                    <div class="col-10">
                        <output class="form-control" type="text" id="example-search-input" name="razaosocial" />
                    </div>
                </div>
                <div class="form-group row">
                    <label for="example-email-input" class="col-2 col-form-label">Bairro</label>
                    <div class="col-10">
                        <output class="form-control" type="text" id="example-email-input" name="bairro" />
                    </div>
                </div>
                <div class="form-group row">
                    <label for="example-url-input" class="col-2 col-form-label">Cidade</label>
                    <div class="col-10">
                        <output class="form-control" id="example-url-input" name="cidade" />
                    </div>
                </div>
                <div class="form-group row">
                    <label for="example-tel-input" class="col-2 col-form-label">Estado</label>
                    <div class="col-10">
                        <output class="form-control" type="text"  id="example-tel-input" name="estado" />
                    </div>
                </div>
                <div class="form-group row">
                    <label for="example-password-input" class="col-2 col-form-label">Telefone</label>
                    <div class="col-10">
                        <output class="form-control" type="number" id="example-password-input" name="telefone" />
                    </div>
                </div>
                <div class="form-group row">
                    <label for="example-number-input" class="col-2 col-form-label">Email</label>
                    <div class="col-10">
                        <output class="form-control" type="email"  id="example-number-input" name="email" />
                    </div>
                </div>
                <div class="form-group row">
                    <label for="example-number-input" class="col-2 col-form-label">Historico</label>
                    <div class="col-10">
                        <output class="form-control" type="text" id="example-text-input" name="historico" />
                    </div>
                </div>
                <div class="form-group row">
                    <label for="example-number-input" class="col-2 col-form-label">Data</label>
                    <div class="col-10">
                        <output class="form-control" type="date" id="example-date-input" name="data" />
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>
<script src="~/novosite/src/js/pages/cliente/buscarcliente.js"></script>
